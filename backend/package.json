	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coveragePathIgnorePatterns": [
			"/node_modules/",
			"\\.module\\.ts$",
			"\\.model\\.ts$",
			"main\\.ts$",
			"db\\.config\\.ts$",
			"/migrations/",
			"/modules/reminders/",
			"/modules/ai/",
			"/modules/entries/utils/entryRag",
			"/decorators/",
			"/guards/",
			"/logger/"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node",
		"moduleNameMapper": {
			"^src/(.*)$": "<rootDir>/$1"
		}
	}
