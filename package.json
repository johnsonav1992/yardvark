{
    "name": "yardvark",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --configuration production",
        "dev": "ng serve --configuration development",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "update-packages": "npx npm-check-updates -u && npm install -f",
        "auth0:deploy": "node auth0-actions/deploy.js",
        "auth0:deploy:single": "cd auth0-actions && auth0 actions deploy",
        "ios:deploy": "npm run build && npx cap sync ios && npx cap open ios",
        "ios": "npm run build && npx cap sync ios && npx cap open ios && npm run dev -- --host 0.0.0.0 --ssl true"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^21.0.3",
        "@angular/cdk": "^21.0.1",
        "@angular/common": "^21.0.3",
        "@angular/compiler": "^21.0.3",
        "@angular/core": "^21.0.3",
        "@angular/forms": "^21.0.3",
        "@angular/platform-browser": "^21.0.3",
        "@angular/platform-browser-dynamic": "^21.0.3",
        "@angular/router": "^21.0.3",
        "@angular/service-worker": "^21.0.3",
        "@auth0/auth0-angular": "^2.2.3",
        "@capacitor/angular": "^2.0.3",
        "@capacitor/app": "^7.1.0",
        "@capacitor/browser": "^7.0.2",
        "@capacitor/core": "^7.4.3",
        "@capacitor/ios": "^7.4.3",
        "@capacitor/splash-screen": "^8.0.0",
        "@geoman-io/leaflet-geoman-free": "^2.19.0",
        "@primeng/themes": "^19.1.3",
        "@primeuix/themes": "^1.0.0",
        "@tabler/icons-webfont": "^3.34.0",
        "@types/leaflet": "^1.9.21",
        "chart.js": "^4.4.9",
        "chartjs-plugin-datalabels": "^2.2.0",
        "date-fns": "^4.1.0",
        "leaflet": "^1.9.4",
        "logrocket": "^10.0.0",
        "primeng": "^21.0.2",
        "rxjs": "~7.8.2",
        "tslib": "^2.8.1"
    },
    "devDependencies": {
        "@angular/build": "^21.0.3",
        "@angular/cli": "^21.0.3",
        "@angular/compiler-cli": "^21.0.3",
        "@capacitor/assets": "^3.0.5",
        "@capacitor/cli": "^7.4.3",
        "@types/chart.js": "^4.0.1",
        "@types/jasmine": "~5.1.8",
        "jasmine-core": "~5.7.1",
        "karma": "~6.4.4",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.1",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "prettier": "^3.5.3",
        "typescript": "~5.9.0"
    }
}