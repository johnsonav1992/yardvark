# AWS S3 Configuration
AWS_ACCESS_KEY_ID_YARDVARK=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY_YARDVARK=your_aws_secret_key_here
AWS_REGION_YARDVARK=us-east-1
AWS_S3_BUCKET_YARDVARK=your_bucket_name_here

# AI Service API Keys
